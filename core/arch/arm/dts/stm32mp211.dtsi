			scmi_reset: protocol@16 {
				reg = <0x16>;

				resets {
					#address-cells = <1>;
					#size-cells = <0>;
					#reset-cells = <1>;

					reset@0 {
						reg = <RST_SCMI_C1_R>;
						domain-name = "c1";
						resets = <&rcc C1_R>;
					};

					reset@1 {
						reg = <RST_SCMI_C2_R>;
						domain-name = "c2";
						resets = <&m33_rproc 0>;
					};

					reset@2 {
						reg = <RST_SCMI_C1_HOLDBOOT_R>;
						domain-name = "c1_holdboot";
						resets = <&rcc C1_HOLDBOOT_R>;
					};

					reset@3 {
						reg = <RST_SCMI_C2_HOLDBOOT_R>;
						domain-name = "c2_holdboot";
						resets = <&m33_rproc 1>;
					};

					reset@4 {
						reg = <RST_SCMI_FMC>;
						domain-name = "fmc";
						resets = <&rcc FMC_R>;
					};

					reset@5 {
						reg = <RST_SCMI_OSPI1>;
						domain-name = "ospi1";
						resets = <&rcc OSPI1_R>;
					};

					reset@6 {
						reg = <RST_SCMI_OSPI1DLL>;
						domain-name = "ospi1_ddl";
						resets = <&rcc OSPI1DLL_R>;
					};
				};
			};

			scmi_regulator: protocol@17 {
				reg = <0x17>;

				scmi_regu: regulators {
					#address-cells = <1>;
					#size-cells = <0>;

					voltd-vddio1 {
						reg = <VOLTD_SCMI_VDDIO1>;
						voltd-supply = <&vddio1>;
					};

					voltd-vddio2 {
						reg = <VOLTD_SCMI_VDDIO2>;
