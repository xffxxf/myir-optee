				 <4 0 0x80000000 0x10000000>; /* NAND */
		};
	};

	soc1: soc@1 {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		interrupt-parent = <&intc>;
		ranges = <0x0 0x0 0x0 0x80000000>;
	};
};
